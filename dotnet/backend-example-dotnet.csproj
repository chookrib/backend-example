<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <RootNamespace>BackendExample</RootNamespace>
        <!-- 注入编译时间 -->
        <!--<BuildTime>$([System.DateTime]::UtcNow.ToString("yyyy-MM-dd HH:mm:ss"))</BuildTime>-->
        <BuildTime>$([System.DateTime]::Now.ToString("yyyyMMddHHmmss"))</BuildTime>
    </PropertyGroup>

    <ItemGroup>
        <!-- 注入编译时间 -->
        <AssemblyMetadata Include="BuildTime" Value="$(BuildTime)" />
        <PackageReference Include="Dapper" Version="2.1.66" />
        <PackageReference Include="log4net" Version="3.2.0" />
        <PackageReference Include="Microsoft.Data.Sqlite" Version="9.0.10" />
        <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="8.14.0" />
    </ItemGroup>

</Project>
